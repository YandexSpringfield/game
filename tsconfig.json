{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "esnext"],
    "esModuleInterop": true
  },
  "extends": "./tsconfig.paths.json",
  // "include": ["./client/**/*.ts", "./client/**/*.tsx"],
  "exclude": ["node_modules"]
}
