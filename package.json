{
  "name": "game",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:production",
    "init": "npm install && npm run build:dev:vendors",
    "dev": "better-npm-run dev",
    "build:production:vendors": "better-npm-run build:production:vendors",
    "build:production:client": "better-npm-run build:production:client",
    "build:production": "better-npm-run build:production",
    "build:client": "better-npm-run build:client",
    "watch:ssr": "better-npm-run watch:ssr",
    "start:dev": "NODE_PATH=./dist CFG_DIR=$(pwd)/dist/configs node dist/server/index.js",
    "start:prod": "cd /var/www/html && APP_VERSION=$(cat $(pwd)/.version) CFG_DIR=$(pwd)/dist/configs/ NODE_PATH=$(pwd)/dist node dist/server/index.js",
    "build:configs": "tsc --project configs",
    "build:server": "tsc --project server",
    "build:dev:vendors": "NODE_ENV=development webpack --mode=development --config webpack/config/vendors.config.ts",
    "build:ssr": "webpack --mode=production --config webpack/config/ssr.config.ts",
    "clean": "rm -rf dist && mkdir dist",
    "precommit": "npm-run-all lint tsc",
    "tsc": "node ./node_modules/.bin/tsc --noEmit",
    "lint": "npm-run-all lint:*",
    "lint:style": "npx stylelint './**/*.{css,scss}'",
    "lint:src": "npx eslint --ext=.ts,.tsx ./",
    "fix": "npm-run-all fix:lint:*",
    "fix:lint:src": "npx eslint --fix --ext=.ts,.tsx ./",
    "fix:lint:style": "npx stylelint './**/*.{css,scss}' --fix",
    "test": "npm-run-all lint:* test:*",
    "test:mocha": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YandexSpringfield/game.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YandexSpringfield/game/issues"
  },
  "homepage": "https://github.com/YandexSpringfield/game#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-react": "7.16.5",
    "@babel/preset-typescript": "7.16.5",
    "@svgr/webpack": "6.1.2",
    "@types/classnames": "2.3.1",
    "@types/copy-webpack-plugin": "10.1.0",
    "@types/express": "4.16.1",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.20",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "babel-loader": "8.2.3",
    "better-npm-run": "0.1.1",
    "copy-webpack-plugin": "6.2.1",
    "css-loader": "6.5.1",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "19.0.2",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.7",
    "npm-run-all": "4.1.5",
    "path": "0.12.7",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.0.2",
    "prettier": "2.5.1",
    "prettier-eslint": "8.2.2",
    "react-test-renderer": "17.0.2",
    "sass": "1.45.0",
    "sass-loader": "12.4.0",
    "style-loader": "3.3.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.3",
    "workbox-webpack-plugin": "6.4.2"
  },
  "dependencies": {
    "@hot-loader/react-dom": "17.0.2",
    "@reduxjs/toolkit": "1.7.1",
    "axios": "^0.24.0",
    "classnames": "2.3.1",
    "compression-webpack-plugin": "4.0.0",
    "cookie-parser": "1.4.6",
    "dotenv": "8.2.0",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "express": "4.17.1",
    "express-rate-limit": "6.2.0",
    "fork-ts-checker-webpack-plugin": "7.0.0",
    "helmet": "5.0.2",
    "htmlescape": "1.1.1",
    "lodash-webpack-plugin": "0.11.6",
    "lodash.flow": "3.5.0",
    "lodash.merge": "4.6.2",
    "merge-options": "3.0.3",
    "mini-css-extract-plugin": "2.5.3",
    "pkg-dir": "4.2.0",
    "qs": "6.10.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-hot-loader": "4.13.0",
    "react-loadable": "5.5.0",
    "react-redux": "7.2.6",
    "react-router-dom": "6.1.1",
    "redux-thunk": "2.4.1",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript-plugin-styled-components": "2.0.0",
    "webpack": "5.65.0",
    "webpack-dev-middleware": "5.3.0",
    "webpack-hot-middleware": "2.25.1",
    "webpack-node-externals": "3.0.0"
  },
  "betterScripts": {
    "dev": {
      "command": "npm-run-all build:configs build:server && npm-run-all --parallel --print-name watch:ssr start:dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:production": {
      "command": "npm-run-all build:configs build:server build:production:vendors build:ssr build:production:client",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:production:vendors": {
      "command": "webpack --mode=production --config webpack/config/vendors.config.ts",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:production:client": {
      "command": "webpack --mode=production --config webpack/config/client.config.ts",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "watch:ssr": {
      "command": "webpack --watch none --config webpack/config/ssr.config.ts",
      "env": {
        "NODE_ENV": "development"
      }
    }
  }
}
